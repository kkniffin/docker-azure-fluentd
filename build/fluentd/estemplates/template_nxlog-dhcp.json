{
	"template": "dhcp-*",
	"settings": {
		"index.refresh_interval": "60s"
	},
	"mappings": {
		"_default_": {
			"_all": {
				"enabled": false
			},
			"dynamic_templates" : [{
        	      		"message_field" : {
                			"match" : "message",
                			"match_mapping_type" : "string",
                			"mapping" : { "type" : "string", "index" : "not_analyzed" }
	              		}
        	    	},
			{
              			"string_fields" : {
                			"match" : "*",
                			"match_mapping_type" : "string",
	                		"mapping" : { "type" : "string", "index" : "not_analyzed" }
        	      		}
            		}]
		},
		"fluentd": {
			"_all": {
        			"enabled": true
      			},
			"properties": {
				"Date": {
					"type": "date",
					"format": "MM/dd/yyyy"
				},
				"EventReceivedTime": {
					"type": "date",
					"format": "yyyy-MM-dd HH:mm:ss||yyyy-MM-dd HH:mm:ss Z"
				},
				"Description": {
                                        "type": "string",
                                        "index" : "analyzed"
                                },
				"IDDef": {
                                        "type": "string"
                                },
				"Message": {
                                        "type": "string",
                                        "index" : "analyzed"
                                },
				"IPAddress": {
					"type": "ip"
				},
				"Time": {
					"type": "date",
					"format": "HH:mm:ss"
				},
				"SyslogReceived": {
					"type": "date",
					"format": "yyyy-MM-dd HH:mm:ss||yyyy-MM-dd HH:mm:ss Z"
				}
			}
		}
	}
}
