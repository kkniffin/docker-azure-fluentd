{
	"template": "dns-*",
	"settings": {
		"index.refresh_interval": "60s"
	},
	"mappings": {
		"_default_": {
			"_all": {
				"enabled": true
			},
			"dynamic_templates" : [{
        	      		"message_field" : {
                			"match" : "message",
                			"match_mapping_type" : "string",
                			"mapping" : { "type" : "string", "index" : "not_analyzed" }
	              		}
        	    	},
			{
              			"string_fields" : {
                			"match" : "*",
                			"match_mapping_type" : "string",
	                		"mapping" : { "type" : "string", "index" : "not_analyzed" }
        	      		}
            		}]
		},
		"fluentd": {
			"_all": {
        			"enabled": true
      			},
			"properties": {
				"DateTime": {
					"type": "date",
					"format": "MM/dd/yyyy HH:mm:ss a"
				},
				"EventReceivedTime": {
					"type": "date",
					"format": "yyyy-MM-dd HH:mm:ss||yyyy-MM-dd HH:mm:ss Z"
				},
				"QuestionName": {
					"type": "string",
					"index": "not_analyzed",
					"fields": {
						"analyzed": {
							"type": "string",
							"index": "analyzed"
						}
					}
				},
				"RemoteIP": {
					"type": "ip"
				},
				"SyslogReceived": {
					"type": "date",
					"format": "yyyy-MM-dd HH:mm:ss||yyyy-MM-dd HH:mm:ss Z"
				}
			}
		}
	}
}
